{
  "MATSys": {
    "EnableNLogInJsonFile": true,
    "Transceiver": {
      "Type": "",
      "AliasName": "",
      "LocalIP": "tcp://127.0.0.1",
      "Port": 1234
    },
    "Modules": [
      {
        "Name": "Dev1",
        "Type": "TestDevice",
      }
    ],
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "./${shortdate}.log"
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "none",
        "minLevel": "Debug",
        "writeTo": "logfile"
      }
    ]
  }
}