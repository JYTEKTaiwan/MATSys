<?xml version="1.0" encoding="utf-8" ?>
<!-- XSD manual extracted from package NLog.Schema: https://www.nuget.org/packages/NLog.Schema-->
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xsi:schemaLocation="NLog NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true">
	<!--internalLogFile="c:\temp\console-example-internal.log"-->
	<!--internalLogLevel="Info" -->

	<!-- the targets to write to -->
	<targets>
		<!-- write logs to file -->
		<!--<target xsi:type="File" name="loginfo" fileName="loginfo.txt"
				layout="${longdate}|${level}|${loggername}|${message} " />
		<target xsi:type="File" name="logdebug" fileName="logdebug.txt"
				layout="${longdate}|${level}|${loggername}|${message} " />
		<target xsi:type="File" name="logtrace" fileName="logtrace.txt"
				layout="${longdate}|${level}|${loggername}|${message} " />-->
		<target xsi:type="File" name="log" fileName="log.txt"
				layout="${longdate}|${level}|${loggername}|${message} " />
	</targets>

	<!-- rules to map from logger name to target -->
	<rules>
		<!--<logger name="*" level="Info" writeTo="loginfo" />
		<logger name="*" level="Debug" writeTo="logdebug" />
		<logger name="*" level="Trace" writeTo="logtrace" />-->
		<logger name="*" minlevel="Trace" writeTo="log" />
	</rules>
</nlog>