{
  "MATSys": {
    "Modules": [
      {
        "AssemblyPath": "",
        "Alias": "Dev1",
        "Type": "InteractionWithHost.TestDevice"
      }       
    ],    
    "Runner": {
      "Type": "SriptRunner",
      "RootDirectory": ".\\scripts",
      "Setup": [ { "Dev1": { "Method": [ "Setup" ] } } ],
      "Test": [ { "Script": "maintest.ats" } ],
      "Teardown": [ {  "Dev1": { "Method": [ "Teardown" ] }  } ]
    }
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "./${shortdate}.log"
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "logfile"
      }
    ]
  }
}