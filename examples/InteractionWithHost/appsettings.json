{
  "MATSys": {
    "Modules": [
      {
        "Alias": "Dev1",
        "Type": "TestDevice",
        "Notifier": {
          "Type": "netmq",
          "Address": "127.0.0.1:12345",
          "Protocal": "inproc",
          "Topic": "AA",
          "DisableEvent": false
        },
        "Transceiver": {
          "Type": "netmq",
          "AliasName": "",
          "LocalIP": "tcp://127.0.0.1",
          "Port": 1234
        }
      }

    ],
    "References": {
      "Modules": [],
      "Recorders": [ ".\\modules\\CSVRecorder.dll" ],
      "Notifiers": [ ".\\modules\\NetMQNotifier.dll" ],
      "Transceivers": [ ".\\modules\\NetMQTransceiver.dll" ]
    },
    "ScriptMode": true,
    "Scripts": {
      "RootDirectory": ".\\scripts",
      "Setup": [ { "Executer": { "Dev1": { "Method": [ "Setup" ] } } } ],
      "Test": [ { "Script": "maintest.ats" } ],
      "Teardown": [ { "Executer": { "Dev1": { "Method": [ "Teardown" ] } } } ]
    }
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "./${shortdate}.log"
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "none",
        "minLevel": "Trace",
        "writeTo": "logfile"
      }
    ]
  }
}